{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "schemas/error-schema.json",
  "title": "error",
  "type": "object",
  "properties": {
    "code" : {
      "type" : "string",
      "enum" : [ "BadRequest", "InvalidArgs", "FetchFailed", "CriticalFailure" ]

    },
    "errorMessage" : {
      "type" : "string"
    }
  },

  "required" : ["code","errorMessage"]
}


